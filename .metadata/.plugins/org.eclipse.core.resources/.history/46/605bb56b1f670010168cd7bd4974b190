package com.techlabs.model;

public class Bassguitar extends Guitar{

	public Bassguitar() {
		playBehaviour = new PlayWithFingers();
		tuneBehaviour = new OpenTuning();
		effectBehaviour = new ChorusEffect();
	}
	
	@Override
	public void display() {
		System.out.println("\n\n----Bass Guitar----");
	}

}
